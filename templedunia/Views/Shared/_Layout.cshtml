    <title>TempleDuniya - @ViewBag.Title</title>
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/assets/Images/TempleImages/ico.ico">

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="../../css2?family=Nunito+Sans:wght@200;300;400;600;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="../../font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="../../ajax/libs/ionicons/4.5.6/css/ionicons.min.css">
